<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!--导入属性配置 -->
	<properties resource="mybatis-generator.properties" />
	<context id="test" targetRuntime="MyBatis3">
		
		<!--分割SQL关键字 默认为false -->
		<property name="autoDelimitKeywords" value="true" />
		<!--分割关键字的前符号 -->
		<property name="beginningDelimiter" value="`" />
		<!--分割关键字的后符号 -->
		<property name="endingDelimiter" value="`" />
	
		<plugin type="org.mybatis.generator.my.plugins.DynamicQueryExamplePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.PaginationPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.ExampleEnhancedPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.ModelEnhancedPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.XmlFileOverwritePlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.InsertBatchPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.UpdateBatchPlugin"></plugin>
		<plugin type="org.mybatis.generator.my.plugins.MapperPlugin"></plugin>

		<!-- type="org.mybatis.generator.my.plugins.TableCommentGenerator" -->

		<commentGenerator>
			<!-- 这个元素用来去除指定生成的注释中是否包含生成的日期 false:表示保护 -->
			<!-- 如果生成日期，会造成即使修改一个字段，整个实体类所有属性都会发生变化，不利于版本控制，所以设置为true -->
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="${jdbc.driver}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
		</jdbcConnection>

		<javaTypeResolver>
			<!-- This property is used to specify whether MyBatis Generator should 
				force the use of java.math.BigDecimal for DECIMAL and NUMERIC fields, -->
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>


		<!--生成Model类存放位置 -->
		<javaModelGenerator targetPackage="${model.package}"
			targetProject="${model.target.project}">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!--生成映射文件存放位置 -->
		<sqlMapGenerator targetPackage="${xml.mapper.package}"
			targetProject="${xml.target.project}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!--生成Dao类存放位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="${dao.package}" targetProject="${dao.target.project}">
			<property name="enableSubPackages" value="true" />
			<property name="rootInterface" value="org.mybatis.generator.my.mapper.Mapper" />
		</javaClientGenerator>

		<!-- <table schema="oms" tableName="application" domainObjectName="Application"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="app_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		<table schema="oms" tableName="application" domainObjectName="Application"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="app_id" sqlStatement="JDBC"
				identity="true" />
			<columnOverride column="is_delete" javaType="Boolean" />
		</table>
		<table schema="oms" tableName="application_link"
			domainObjectName="ApplicationLink" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="false">
			<generatedKey column="link_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		<table schema="oms" tableName="order" domainObjectName="Order"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="order_id" sqlStatement="JDBC"
				identity="true" />
			<columnOverride column="is_delete" javaType="Boolean" />
		</table>
		<table schema="oms" tableName="order_address" domainObjectName="OrderAddress"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="address_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		<table schema="oms" tableName="order_cargo" domainObjectName="OrderCargo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="cargo_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		<table schema="oms" tableName="order_optimize" domainObjectName="OrderOptimize"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="optimize_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		<table schema="oms" tableName="order_text" domainObjectName="OrderText"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="text_id" sqlStatement="JDBC"
				identity="true" />
		</table>

		<table schema="oms" tableName="order" domainObjectName="Order"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="order_id" sqlStatement="JDBC"
				identity="true" />
			<columnOverride column="is_delete" javaType="Boolean" />
		</table>

		<table schema="oms" tableName="optimize_param"
			domainObjectName="OptimizeParam" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="false">
			<generatedKey column="param_id" sqlStatement="JDBC"
				identity="true" />
		</table>
		
		<table schema="oms" tableName="order" domainObjectName="Order"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="order_id" sqlStatement="JDBC"
				identity="true" />
			<columnOverride column="is_delete" javaType="Boolean" />
		</table>
		
		-->
		<table schema="oms" tableName="order" domainObjectName="Order"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="false">
			<generatedKey column="order_id" sqlStatement="JDBC"
				identity="true" />
